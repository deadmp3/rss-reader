{
  "name": "rss-reader",
  "version": "0.0.1",
  "description": "RSS reader",
  "scripts": {
    "nodemon": "nodemon",
    "babel-node": "babel-node",
    "eslint": "eslint",
    "webpack": "webpack",
    "start": "node dist/bin/rss-reader.js",
    "build-app": "NODE_ENV=production npm run webpack -- -p",
    "build-server": "babel server --out-dir dist --source-maps inline",
    "build": "npm run build-app && npm run build-server",
    "postinstall": "rm -rf dist && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deadmp3/rss-reader.git"
  },
  "keywords": [
    "rss",
    "reader"
  ],
  "author": "Sergey Grigoryev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deadmp3/rss-reader/issues"
  },
  "homepage": "https://github.com/deadmp3/rss-reader#readme",
  "dependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-preset-env": "*",
    "babel-preset-stage-0": "*",
    "babel-polyfill": "*",
    "koa": "*",
    "koa-bodyparser": "*",
    "koa-router": "*",
    "koa-static": "*",
    "koa-webpack": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "html-webpack-plugin": "*",
    "copy-webpack-plugin": "*",
    "bootstrap": "4.1.1",
    "jquery": "*",
    "popper.js": "*",
    "style-loader": "*",
    "css-loader": "*",
    "validator": "*",
    "axios": "*",
    "uuid": "*",
    "mz": "*"
  },
  "devDependencies": {
    "babel-eslint": "*",
    "babel-jest": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-plugin-babel": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jest": "*",
    "jest": "*",
    "nodemon": "*",
    "nanodelay": "*"
  }
}
